	#include <stdio.h>

int estPremier(int x) {
    if (x < 2) return 0; // 0 et 1 ne sont pas premiers

    for (int i = 2; i*i <= x; i++) {
        if (x % i == 0)
            return 0; // Pas premier
    }

    return 1; // Premier
}

int main() {
    int n;

    printf("Entrez un nombre inferieur a 100 : ");
    scanf("%d", &n);

    if (n <= 1 || n >= 100) {
        printf("Veuillez entrer un nombre entre 2 et 99.\n");
        return 0;
    }
    printf("Les nombres de 1 jusqu'a %d :\n", n);
    for (int i = 1; i <= n; i++) {
        printf("%d ", i);
    }

    printf("\n\nLes nombres premiers entre 1 et %d :\n", n);
    for (int i = 1; i <= n; i++) {
        if (estPremier(i))
            printf("%d ", i);
    }

    printf("\n");
    return 0;
}